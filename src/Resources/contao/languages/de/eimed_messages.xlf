          target-language="de">
        <body>
            <trans-unit id="eimed.pw_policy_missmatch_message">
                <source>The password should</source>
                <target>Das Passwort muss</target>
            </trans-unit>
            <trans-unit id="eimed.pw_min_length_message">
                <source>be at least %s characters long</source>
                <target>mindestens %s Zeichen lang sein</target>
            </trans-unit>
            <trans-unit id="eimed.pw_max_length_message">
                <source>be maximum %s characters long</source>
                <target>höchstens %s Zeichen lang sein</target>
            </trans-unit>
            <trans-unit id="eimed.pw_upper_case_message">
                <source>have at least %s upper-case character(s)</source>
                <target>mindestens %s Großbuchstaben enthalten</target>
            </trans-unit>
            <trans-unit id="eimed.pw_lower_case_message">
                <source>have at least %s lower-case character(s)</source>
                <target>mindestens %s Kleinbuchstaben enthalten</target>
            </trans-unit>
            <trans-unit id="eimed.pw_special_chars_message">
                <source>have at least %s special-character(s)</source>
                <target>mindestens %s Sonderzeichen enthalten</target>
            </trans-unit>
            <trans-unit id="eimed.pw_digits_message">
                <source>have at least %s digit(s)</source>
                <target>mindestens %s Ziffer(n) enthalten</target>
            </trans-unit>
        </body>
